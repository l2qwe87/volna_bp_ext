		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	НоменИДаты.Ссылка КАК Заказ,
	МИНИМУМ(НоменИДаты.Значение) КАК ДатаЗакрытияМин,
	МИНИМУМ(ВЫБОР
			КОГДА НоменИДаты.Свойство.Заголовок = "ДатаЗакрытия"
				ТОГДА НоменИДаты.Значение
			ИНАЧЕ NULL
		КОНЕЦ) КАК ДатаЗакрытия,
	МИНИМУМ(ВЫБОР
			КОГДА НоменИДаты.Свойство.Заголовок = "ДатаЗавершенияПроизводства"
				ТОГДА НоменИДаты.Значение
			ИНАЧЕ NULL
		КОНЕЦ) КАК ДатаЗавершенияПроизводства,
	МИНИМУМ(ВЫБОР
			КОГДА НоменИДаты.Свойство.Заголовок = "ДатаРеализации"
				ТОГДА НоменИДаты.Значение
			ИНАЧЕ NULL
		КОНЕЦ) КАК ДатаРеализации
ПОМЕСТИТЬ ВТ_НоменИДаты
ИЗ
	Справочник.НоменклатурныеГруппы.ДополнительныеРеквизиты КАК НоменИДаты
ГДЕ
	НоменИДаты.Свойство.Заголовок В ("ДатаЗавершенияПроизводства", "ДатаЗакрытия", "ДатаРеализации")
	И НоменИДаты.Значение &gt; ДАТАВРЕМЯ(1990, 1, 1)

СГРУППИРОВАТЬ ПО
	НоменИДаты.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	"ТребованиеНакладная" КАК ТипДокумента,
	Треб.Ссылка КАК Документ,
	Треб.Ссылка.Дата КАК ДатаДокумента,
	Треб.Субконто1 КАК Заказ,
	Треб.СчетЗатрат КАК СчетЗатрат,
	НоменИДаты.ДатаЗавершенияПроизводства КАК ДатаЗавершенияПроизводства,
	НоменИДаты.ДатаЗакрытия КАК ДатаЗакрытия,
	НоменИДаты.ДатаРеализации КАК ДатаРеализации
ИЗ
	Документ.ТребованиеНакладная КАК Треб
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_НоменИДаты КАК НоменИДаты
		ПО (НоменИДаты.Заказ = Треб.Субконто1)
			И (НАЧАЛОПЕРИОДА(Треб.Дата, ДЕНЬ) &gt; НоменИДаты.ДатаЗакрытияМин)
ГДЕ
	Треб.Проведен
	И Треб.Дата &gt;= &amp;ДатаН
	И Треб.Дата &lt;= &amp;ДатаК

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Док.ТипДокумента,
	Док.Ссылка,
	Док.Ссылка.Дата,
	Док.Субконто1,
	Док.СчетЗатрат,
	НоменИДаты.ДатаЗавершенияПроизводства,
	НоменИДаты.ДатаЗакрытия,
	НоменИДаты.ДатаРеализации
ИЗ
	(ВЫБРАТЬ РАЗЛИЧНЫЕ
		"АвансовыйОтчет" КАК ТипДокумента,
		Т.Ссылка КАК Ссылка,
		Т.Ссылка.Проведен КАК Проведен,
		Т.Ссылка.Дата КАК Дата,
		Т.Субконто1 КАК Субконто1,
		Т.СчетЗатрат КАК СчетЗатрат
	ИЗ
		Документ.АвансовыйОтчет.Прочее КАК Т
	ГДЕ
		Т.Ссылка.Проведен
		И Т.Ссылка.Дата &gt;= &amp;ДатаН
		И Т.Ссылка.Дата &lt;= &amp;ДатаК
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		"Заработная плата",
		ХозрасчетныйОбороты.Регистратор,
		ИСТИНА,
		НАЧАЛОПЕРИОДА(ХозрасчетныйОбороты.Период, МЕСЯЦ),
		ХозрасчетныйОбороты.Субконто1,
		ХозрасчетныйОбороты.Счет
	ИЗ
		РегистрБухгалтерии.Хозрасчетный.Обороты(&amp;ДатаН, &amp;ДатаК, Регистратор, Счет.Код В ("20.01"), , Субконто2.Наименование = "Основная заработная плата производственного персонала", , ) КАК ХозрасчетныйОбороты
	
	СГРУППИРОВАТЬ ПО
		ХозрасчетныйОбороты.Регистратор,
		ХозрасчетныйОбороты.Счет,
		ХозрасчетныйОбороты.Субконто1,
		НАЧАЛОПЕРИОДА(ХозрасчетныйОбороты.Период, МЕСЯЦ)
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ РАЗЛИЧНЫЕ
		"ПоступлениеТоваровУслуг",
		Т.Ссылка,
		Т.Ссылка.Проведен,
		Т.Ссылка.Дата,
		Т.Субконто1,
		Т.СчетЗатрат
	ИЗ
		Документ.ПоступлениеТоваровУслуг.Услуги КАК Т
	ГДЕ
		Т.Ссылка.Проведен
		И Т.Ссылка.Дата &gt;= &amp;ДатаН
		И Т.Ссылка.Дата &lt;= &amp;ДатаК) КАК Док
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_НоменИДаты КАК НоменИДаты
		ПО (НоменИДаты.Заказ = Док.Субконто1)
			И (НАЧАЛОПЕРИОДА(Док.Дата, ДЕНЬ) &gt; НоменИДаты.ДатаЗакрытияМин)</query>
	</dataSet>
